RewriteEngine     On

RewriteRule ^([0-9]{2}[0-9]{2}[0-9]{2})/?$  index.php?label=$1   [L]
RewriteRule ^([0-9]{2}[0-9]{2}[0-9]{2})/p([0-9]+)/?$  index.php?label=$1&page=$2   [L]
RewriteRule ^([0-9]{2}[0-9]{2}[0-9]{2}[0-9]{2})/([^/]+)/?$  index.php?date=$1&slug=$2   [L]
RewriteRule ^p([0-9]+)/?$  index.php?page=$1   [L]
RewriteRule ^rss/?$  index.php?output=rss   [L]